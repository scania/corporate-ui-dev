{
  "name": "corporate-ui-dev",
  "version": "4.0.0-alpha.2",
  "description": "In this repository you'll find all components available in the UX Library",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/scania/corporate-ui-dev.git"
  },
  "module": "dist/index.js",
  "main": "dist/index.js",
  "collection": "dist/collection/collection-manifest.json",
  "files": [
    "dist"
  ],
  "scripts": {
    "start": "gulp",
    "build": "gulp build",
    "release": "gulp release",
    "clean": "gulp clean",
    "server": "gulp server",
    "storybook": "start-storybook -p 1337 -c public",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "lint-fix": "npm run lint -- --fix",
    "lint-debug": "npm run lint -- --debug",
    "test": "stencil test --spec --e2e",
    "test.watch": "stencil test --spec --e2e --watchAll"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "git add"
    ]
  },
